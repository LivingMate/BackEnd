{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": ["esnext"],
    "esModuleInterop": true,
    "types": ["@prisma/client"],
    "target": "ESNext",
    "module": "ESNext",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": ".",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "pretty": true,
    "allowJs": true,
    "resolveJsonModule": true,
  },
  "ts-node": {
    "esm":true
  },
  "include": ["*.ts"],
  "exclude": ["node_modules"]
}