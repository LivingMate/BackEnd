{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": ["esnext"],
    "esModuleInterop": true,
    "types": ["@prisma/client"],
    "target": "es2020",
    "module": "commonjs",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src",
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
